{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-flow",
    "plugin:react/recommended"
  ],
  "globals": {
    "sails": false
  },
  "plugins": [ "flowtype" ],
  "rules": {
    "no-useless-constructor": 1,
    "comma-dangle": 0
  },
  "globals": {
    "__DEV__": false,
    "logger": false
  }
}
